{
  "name": "dotenv-master",
  "version": "1.0.1",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "dotenv-master": "dist/cli/cli.js"
  },
  "scripts": {
    "dev": "npm run build && node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
